{
  "name": "@twada/specmd-reporter",
  "version": "0.0.0",
  "description": "Node Test reporter that generates API spec description for README",
  "keywords": [
    "node:test",
    "reporter"
  ],
  "license": "MIT",
  "author": {
    "name": "Takuto WADA",
    "email": "takuto.wada@gmail.com",
    "url": "https://github.com/twada"
  },
  "main": "index.mjs",
  "files": [
    "CHANGELOG.md",
    "README.md",
    "index.js"
  ],
  "scripts": {
    "test": "node --test --test-reporter=./index.mjs test.mjs | diff -u ./fixture.txt -"
  },
  "publishConfig": {
    "access": "public"
  }
}
